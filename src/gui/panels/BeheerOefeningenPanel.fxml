<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<fx:root id="AnchorPane" prefHeight="400.0" prefWidth="600.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="createOefening" mnemonicParsing="false" onAction="#createOefeningClicked" text="Maak nieuwe oefening">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin></Button>
            <Button fx:id="updateOefening" mnemonicParsing="false" onAction="#updateOefeningClicked" text="Wijzig een oefening">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="deleteOefening" mnemonicParsing="false" onAction="#deleteOefeningClicked" text="Verwijder een oefening">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</fx:root>
